{
  "name": "wkh5_node",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "gulp build",
    "babel_build":"babel src --out-dir dist --source-maps",
    "dev": "pm2 restart pm2.config.js --only ares_dev",
    "devnopm2": "cross-env STAGE_ENV=dev PORT=9015 node ./bin/www",
    "test": "pm2 restart pm2.config.js --only ares_test",
    "testnopm2": "cross-env STAGE_ENV=test PORT=9016 node ./bin/www",
    "sim": "pm2 restart pm2.config.js --only ares_sim",
    "simnopm2": "cross-env STAGE_ENV=sim PORT=9015 node ./bin/www",
    "prod": "pm2 restart pm2.config.js --only ares_prod",
    "prodnopm2": "cross-env STAGE_ENV=prod PORT=9015 node ./bin/www",
    "stopdev": "pm2 stop ares_dev" ,
    "stoptest": "pm2 stop ares_test" ,
    "stopsim": "pm2 stop ares_sim" ,
    "stop": "pm2 stop ares_prod" ,
    "watch": "gulp watch"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "cross-env": "^3.1.3",
    "connect-redis": "^3.1.0",
    "debug": "~2.2.0",
    "express-session": "^1.14.1",
    "require-directory": "^2.1.1",
    "serve-favicon": "~2.3.0",
    "url": "^0.11.0",
    "ejs": "^2.5.5",
    "express": "^4.14.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.0",
    "babel-plugin-syntax-decorators": "^6.13.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-preset-stage-1": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-preset-stage-3": "^6.17.0",
    "captchapng": "0.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
