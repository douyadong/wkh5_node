<div class="conning-tower release">
    <div class="back"><i class="iconfont icon-back"></i></div>
    <div class="hamburg-menu"><i class="iconfont icon-hanbao"></i></div>
    <dl>
        <dt><span><%= cityName %></span><i></i></dt>
        <dd><i class="iconfont icon-search"></i> <span>请输入<% if( moduleType === "xf" ) { %>楼盘<% } else { %>小区<% } %>名称或地址</span></dd>
    </dl>
</div>
<!--点击汉堡菜单弹出的子菜单-->
<div class="navigator-mask"></div>
<div class="navigator-pop">
    <a href="/">首页</a>
    <a href="/<%= cityPinyin %>/esf" <% if( moduleType === "esf" ) { %>class="on"<% } %>>二手房</a>
    <a href="/<%= cityPinyin %>/xflist" <% if( moduleType === "xf" ) { %>class="on"<% } %>>新房</a>
    <a href="/<%= cityPinyin %>/rent" <% if( moduleType === "rent" ) { %>class="on"<% } %>>租房</a>
    <a href="/member/personal.html">个人中心</a>
    <a href="/about.html">关于悟空</a>
    <a class="app-download" href="javascript:void(0);"><i class="iconfont icon-chouTiDown"></i><span>APP下载</span></a>
</div>
<!--点击搜索框区域切换出来的搜索替身-->
<div class="substitute-mask">
    <div class="search-substitute">
        <div class="back"><i class="iconfont icon-back"></i></div>
        <div class="input-section">
            <input placeholder="请输入您想搜索的内容" data-last="">
            <i class="iconfont icon-close"></i>
        </div>
    </div>
    <hr>
    <ul class="list"></ul>    
</div>
<!--城市选择器-->
<div class="city-selector">
    <div class="caption">
        <p>请手动选择城市</p>
    </div>    
</div>
<!--招商页面-->
<div class="notopen-investment hidden">
    <p><img src="<%= appStaticPrefix %>/images/common/notopen.png"></p>
    <p>
        <span class="current-city">石家庄</span>
        <span>的房源正在逐步上线中</span>
    </p>
    <p>
        <span>先看看其他城市的房源</span>
        <a class="change">切换城市</a>
    </p>
    <a href="#" class="apply">城市加盟申请</a>
    <div class="contact">
        <dl>
            <dt><i class="iconfont icon-logotuxing"></i></dt>
            <dd>共享今日头条7亿流量</dd>
            <dd>联系我们：4008215365</dd>
        </dl>
    </div>
</div>
<!--几个隐藏域，fe用来记录最后访问过的城市的cookie就从这里取-->
<input type="hidden" id="visitedCityId" value="<%= cityId %>">
<input type="hidden" id="visitedCityName" value="<%= cityName %>">
<input type="hidden" id="visitedCityPinyin" value="<%= cityPinyin %>">
<input type="hidden" id="visitedCityChina" value="<%= cityChina %>">