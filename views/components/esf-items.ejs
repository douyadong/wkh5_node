<% oldHouseList && oldHouseList.forEach(function(esf){ %>
    <a href="<%= esf.url %>" class="esf-item" data-bigdata="<%= esf.bigDataParams %>">
        <dl>
            <dt><img src="<%= appStaticPrefix %>/images/common/loading.jpg" data-src="<%= esf.houseImgUrl %>?x-oss-process=image/resize,w_150" alt="<%= esf.estateName %>" class="lazy"></dt>
            <dd class="title"><%= esf.houseTitle %></dd>
            <dd><%= esf.houseChild %> <%= esf.areaStr %> | <%= esf.district %> <%= esf.town %></dd>
            <dd class="tags">
                <% if(esf.tagList) { esf.tagList.forEach(function(tag) { %>
                    <span><%= tag %></span>
                <% }) ; } %>                
            </dd>   
            <dd>
                 <span class="money"><%= esf.totalPrice %>万</span>
                 <span class="price"><%= esf.unitPrice %> 元/㎡</span>
            </dd>                             
        </dl>
    </a>
<% }) ; %>