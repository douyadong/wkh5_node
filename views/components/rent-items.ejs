<%# 
    参数：
    {
        rentItems: [{
            ...//别的参数按照参数就好了
            bigDataParams: "", //点击事件埋点
            url: "", //跳转路径
        }]
    }
%>
    <%if(rentItems && rentItems.length > 0){
        for(let i = 0; i < rentItems.length; i++){
            let item = rentItems[i];
    %>        
        <a data-bigdata="<%= item.bigDataParams%>" class="rent-item box" href="<%= item.url%>">
            <div class="left">
                <img data-src="<%= item.firstImageUrl %>" alt="<%= item.estateName %>" class="lazy">
                <%if(item.houseTag.isZeroCommission){%>
                   <span>0 佣金</span>
                <%}%>
            </div>
            <div class="right">
                <h4><%= item.houseTitle%></h4>
                <p class="base-info"><%= item.houseTypeStr%>  <%= item.spaceArea%>㎡ | <%= item.districtAndTownName%></p>
                <p class="base-info"><%= item.distanceSubway%>距3/4/9号线宜山路站500米</p>
                <p class="tags">
                    <%if(item.houseTag){%>
                      <%if(item.houseTag.isSubwayHouse){%><span class="tag nears">近地铁</span><%}%>
                      <%if(item.houseTag.isPriceDown){%><span class="tag priced">降价</span><%}%>
                      <%if(item.houseTag.isNewHouse){%><span class="tag newup">新上</span><%}%>
                      <%if(item.houseTag.isShortRent){%><span class="tag anthert">可短租</span><%}%>
                      <%if(item.houseTag.isHardcover == 1){%><span class="tag anthert">精装</span><%}%>
                      <%if(item.houseTag.isHardcover == 2){%><span class="tag anthert">豪装</span><%}%>
                      <%if(item.houseTag.isSouth){%><span class="tag anthert">朝南</span><%}%>
                    <%}%>
                </p>
                <p class="unit-price">
                    <%= item.rentPriceStr%> 元/月
                </p>
            </div>
        </a>
    <%
        }    
    %>
    <%}%>    
