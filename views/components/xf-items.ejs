<%if(items && items.length){%>
    <%for(var i = 0; i < items.length; i++){%>
        <% let item = items[i]; %>
        <a class="xf-item" href="/<%= citynPinyin%>/xf/<%= item.subEstateId%>.html?channel=<%= channel%>">
            <div class="img">
                <img src="<%= item.imageUrl%>">
                <%if(item.activitys && item.activitys[0]){%>
                    <div class="yh"><%= item.activitys[0].title%></div>
                <%}%>                
            </div>
            <div class="info">
                <h3><%= item.estateName + " " + item.subEstateName%></h3>
                <p class="district-town-area">
                    <span><%= item.districtName + " " + item.townName%></span><span>78m-120m</span>
                </p>
                <ul class="tags">
                    <%if(item.activitys && item.activitys.length){%><li class="yh">有优惠</li><%}%><%if(item.isSubWayEstate){%><li class="dt">近地铁</li><%}%><%if(item.isOnSale){%><li class="">在售楼盘</li><%} else {%><li class="">即将开盘</li><%}%><%if(item.estateVideoResponse){%><li class="">有视频</li><%}%>
                </ul>
                <p class="unit-price"><span><%= item.unitPrice%></span> <span>元/m</span></p>
            </div>
        </a>
    <%}%>    
<%}%>
